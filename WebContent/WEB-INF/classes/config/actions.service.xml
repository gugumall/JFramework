<?xml version="1.0" encoding="UTF-8"?>
<root>	
    <handler path="/j.service.router" path-pattern=".service" class="j.service.router.JRouterImpl" request-by="request">
    	<action id="auth" method="auth" respond-with-string="true">
    		<log avail="false"/>
    	</action> 
    	<action id="heartbeat" method="heartbeat" respond-with-string="true">
    		<log avail="false"/>
    	</action> 
    	<action id="register" method="register" respond-with-string="true">
    		<log avail="false"/>
    	</action>  
    	<action id="unregister" method="unregister" respond-with-string="true">
    		<log avail="false"/>
    	</action> 
    	<action id="service" method="service" respond-with-string="true">
    		<log avail="false"/>
    	</action> 
    	<action id="getAllServiceNodeAvailable" method="getAllServiceNodeAvailable" respond-with-string="true">
    		<log avail="false"/>
    	</action> 
    </handler>
    	
    <handler path="/j.service.hello" path-pattern=".service" class="j.service.hello.JHelloImpl" request-by="request">
    	<action id="auth" method="auth" respond-with-string="true">
    		<log avail="false"/>
    	</action> 
    	<action id="heartbeat" method="heartbeat" respond-with-string="true">
    		<log avail="false"/>
    	</action> 
    	<action id="hello" method="hello" respond-with-string="true"/>  
    </handler>
</root>
