<?xml version="1.0" encoding="UTF-8"?>
<root>		
    <handler path="/I18N" path-pattern=".handler" class="j.I18N.I18N" request-by="request" non-nvwa-obj="true">
    	<action name="保存多语言资源" id="save" method="save" respond-with-string="true">
    	    <log avail="false"></log>
    	</action>
    	<action name="添加多语言资源文件" id="addFile" method="addFile" respond-with-string="true">
    	    <log avail="false"></log>
    	</action>
    	<action name="删除多语言资源文件" id="delFile" method="delFile" respond-with-string="true">
    	    <log avail="false"></log>
    	</action>
    	<action name="添加多语言资源分组" id="addGroup" method="addGroup" respond-with-string="true">
    	    <log avail="false"></log>
    	</action>
    	<action name="删除多语言资源分组" id="delGroup" method="delGroup" respond-with-string="true">
    	    <log avail="false"></log>
    	</action>
    </handler>		
</root>
