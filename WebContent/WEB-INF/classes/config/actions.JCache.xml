<?xml version="1.0" encoding="UTF-8"?>
<root>	
    <handler path="/JCache" path-pattern=".service" class="j.cache.JDCacheServiceImpl" request-by="request">
    	<action id="auth" method="auth" respond-with-string="true"/> 
    	<action id="heartbeat" method="heartbeat" respond-with-string="true"/>
    	
    	<action id="createUnit" method="createUnit" respond-with-string="true"/> 
    	<action id="setActiveTime" method="setActiveTime" respond-with-string="true"/> 
    	<action id="addOne" method="addOne" respond-with-string="true"/> 
    	<action id="addAll" method="addAll" respond-with-string="true"/> 
    	<action id="addOneIfNotContains" method="addOneIfNotContains" respond-with-string="true"/> 
    	<action id="contains" method="contains" respond-with-string="true"/> 
    	<action id="size" method="size" respond-with-string="true"/> 
    	<action id="get" method="get" respond-with-string="true"/> 
    	<action id="remove" method="remove" respond-with-string="true"/> 
    	<action id="clear" method="clear" respond-with-string="true"/> 
    	<action id="update" method="update" respond-with-string="true"/> 
    	<action id="updateCollection" method="updateCollection" respond-with-string="true"/> 
    	<action id="sub" method="sub" respond-with-string="true"/> 
    	<action id="keys" method="keys" respond-with-string="true"/> 
    	<action id="values" method="values" respond-with-string="true"/> 
    </handler>
</root>
