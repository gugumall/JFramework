<?xml version="1.0" encoding="UTF-8"?>
<root>
	<!-- 是否测试环境 -->
	<debug>false</debug>
	
	<!-- 是否路由节点，不是的话删除is-router项 -->
	<is-router>
		<!-- 节点的ID，此处是路由节点 -->
		<property key="j.service.uuid" value="j.service.node.rou:00"/>
	</is-router>
	
	
	<!-- 是否服务节点，不是的话删除is-server项 -->
	<is-server>
		<!-- 节点的ID，此处是服务节点 -->
		<property key="j.service.uuid" value="j.service.node.ser:00"/>
		
		<!-- 用于与服务路由进行MD5校验的约定密钥 -->
		<property key="j.service.key" value="any"/>
	</is-server>
	
	<!-- 是否应用节点，不是的话删除is-client项 -->
	<is-client>	
		<!-- 节点的ID，此处是应用节点 -->
		<property key="j.service.uuid" value="j.service.node.app:sso:00"/>
		
		<!-- 用于与服务路由进行MD5校验的约定密钥 -->
		<property key="j.service.key" value="any"/>
	</is-client>
</root>