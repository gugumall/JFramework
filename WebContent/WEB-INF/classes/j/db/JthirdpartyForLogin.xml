<?xml version="1.0"?>
<!-- <!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >-->
<hibernate-mapping>
<class name="j.db.JthirdpartyForLogin"  table="j_thirdparty_for_login">
<id name="thirdpartyCode" type="java.lang.String" column="THIRDPARTY_CODE">
	<generator class="assigned" />
</id>

<property name="thirdpartyCode" type="java.lang.String" column="THIRDPARTY_CODE" not-null="true" length="64"/>

<property name="thirdpartyName" type="java.lang.String" column="THIRDPARTY_NAME" not-null="false" length="90"/>

<property name="delBySys" type="java.lang.String" column="DEL_BY_SYS" not-null="false" length="1"/>

</class>
</hibernate-mapping>
