<?xml version="1.0"?>
<!-- <!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >-->
<hibernate-mapping>
<class name="j.db.JfsTask"  table="j_fs_task">
<id name="uuid" type="java.lang.String" column="UUID">
	<generator class="assigned" />
</id>

<property name="uuid" type="java.lang.String" column="UUID" not-null="true" length="64"/>

<property name="taskTime" type="java.sql.Timestamp" column="TASK_TIME" not-null="false" length="19"/>

<property name="fromUuid" type="java.lang.String" column="FROM_UUID" not-null="false" length="64"/>

<property name="toUuid" type="java.lang.String" column="TO_UUID" not-null="false" length="64"/>

<property name="filePath" type="java.lang.String" column="FILE_PATH" not-null="false" length="256"/>

<property name="taskOperation" type="java.lang.String" column="TASK_OPERATION" not-null="false" length="64"/>

<property name="taskData" type="java.lang.String" column="TASK_DATA" not-null="false" length="21845"/>

<property name="synTimes" type="java.lang.Integer" column="SYN_TIMES" not-null="false" length="11"/>

<property name="synTime" type="java.sql.Timestamp" column="SYN_TIME" not-null="false" length="19"/>

</class>
</hibernate-mapping>
