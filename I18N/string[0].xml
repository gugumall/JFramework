		</string>
		<string key="管理中心">
			<language code="zh-cn">管理中心</language>
			<language code="zh-tw">管理中心</language>
			<language code="en-us">Management</language>
		</string>
		<string key="刷新">
			<language code="zh-cn">刷新</language>
			<language code="zh-tw">刷新</language>
			<language code="en-us">Refresh</language>
		</string>
